package com.beans;

import org.springframework.stereotype.Component;

@Component    
public class TasasANotificar {
	
	private String numeroPadron;
	private String numeroFactura;
	private String nombreYApellido;
	private String email;
	private Integer importe;
    private String nombreDeTasa;

    public static String EXTENSION_PDF = ".pdf";
	
	public TasasANotificar(String numeroPadron, String numeroFactura, String nombreYApellido, String email, Integer importe, String nombreDeTasa){
		this.numeroPadron = numeroPadron;
		this.numeroFactura = numeroFactura;
		this.nombreYApellido = nombreYApellido;
		this.email = email;
		this.importe = importe;
        this.nombreDeTasa = nombreDeTasa;
	}
	
	public TasasANotificar(){}
	
	public String getNumeroPadron() {
		return numeroPadron;
	}

    public void setNumeroPadron(String numeroPadron) {
		this.numeroPadron = numeroPadron;
	}

    public String getNombreYApellido() {
		return nombreYApellido;
	}

    public void setNombreYApellido(String nombreYApellido) {
		this.nombreYApellido = nombreYApellido;
	}

    public String getNumeroFactura() {
		return numeroFactura;
	}

    public void setNumeroFactura(String numeroFactura) {
		this.numeroFactura = numeroFactura;
	}

    public String getEmail() {
		return email;
	}

    public void setEmail(String email) {
		this.email = email;
	}

	public Integer getImporte() {
		return importe;
	}

	public void setImporte(Integer importe) {
		this.importe = importe;
	}

    public String getNombreDeTasa() {
        return nombreDeTasa;
    }

    public void setNombreDeTasa(String nombreDeTasa) {
        this.nombreDeTasa = nombreDeTasa;
    }
}
